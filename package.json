    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.12.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3"
  },
  "description": ""
}

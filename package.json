{
  "name": "eagles-phantom-backend",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "test": "npm run migrate:reset && npm run seed && cross-env NODE_ENV=test npm run migrate:reset && npm run seed:test && nyc --reporter=html --reporter=text --all=true mocha --require @babel/polyfill --require @babel/register src/test/*.js --timeout 5000000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test-coveralls": "nyc npm run mocha && shx cat ./coverage/lcov.info | node ./bin/coveralls.js --verbose",
    "test:coverage": "jest --coverage",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node  ./src/app.js",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all",
    "migrate:heroku": "npm run migrate:reset &&  npm run seed",
    "clean": "rm -rf build && mkdir build",
    "build-src": "babel -d ./build ./src -s",
    "build": "npm run migrate:heroku && npm run clean && npm run build-src",
    "start": "node --require @babel/polyfill build/app.js"
  },
  "nyc": {
    "exclude": [
      "src/database/seeders",
      "src/database/migrations",
      "coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/eagles-phantom-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/eagles-phantom-backend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/eagles-phantom-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.3.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "ci": "^1.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "i18n": "^0.13.2",
    "jest": "^26.5.2",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "paginate-info": "^1.0.4",
    "pg": "^8.4.0",
    "path": "^0.12.7",
    "pg-hstore": "^2.3.3",
    "rm": "^0.1.8",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
